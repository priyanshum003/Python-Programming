   "source": [
    "def Sum_array(arr):\n",
    "    l = len(arr)\n",
    "    if l == 0:\n",
    "        return 0\n",
    "    smaller_sum = Sum_array(arr[1:])\n",
    "    sum = arr[0] + smaller_sum\n",
    "    return sum\n"
   ]
