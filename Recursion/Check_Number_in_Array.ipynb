    "def checknumber(arr,x):\n",
    "    l = len(arr)\n",
    "    if l == 0:\n",
    "        return False\n",
    "    if arr[0] == x :\n",
    "        return True\n",
    "    \n",
    "    return checknumber(arr[1:],x)\n"
   ]
